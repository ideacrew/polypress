<% if account_signed_in? %>
  <li class="nav-item active">
    <%= link_to "Sign Out #{current_account.email}", destroy_account_session_path, method: :delete, class: 'nav-link' %>
  </li>
  <% else %>
    <li class="nav-item active">
      <%= link_to 'Sign In', new_account_session_path, class: 'nav-link' %>
    </li>
    <li class="nav-item active">
      <%= link_to 'Sign Up', new_account_registration_path, hidden:true, class: 'nav-link' %>
    </li>
    <% end %>